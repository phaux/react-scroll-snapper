{
  "compilerOptions": {
    "exactOptionalPropertyTypes": true,
    "jsx": "react-jsx",
    "lib": ["ESNext", "DOM", "DOM.Iterable"],
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "noUncheckedIndexedAccess": true,
    "plugins": [{ "name": "typescript-plugin-css-modules" }],
    "strict": true,
    "target": "ESNext"
  }
}
